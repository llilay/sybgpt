<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>上传实验报告</title>
	<meta name="renderer" content="webkit">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
	<link rel="stylesheet" href="__PUBLIC__/layui/css/layui.css" media="all">
	<link rel="stylesheet" href="__PUBLIC__/Home/css/base.css" media="all">
	<style type="text/css">
		td{line-height:30px;}
		tr>td{text-indent:20px;}
		.layui-elem-quote{margin-top: 30px;}
		td>a,td>a:hover{color:#FF5722;}
	</style>
</head>
<body>
<div class="layui-fluid main">
	<blockquote class="layui-elem-quote layui-quote-nm">
		<script>
			if(localStorage.role_id == 6){
			    document.write("<a class='layui-btn layui-btn-sm' href='__ROOT__/Home/Report/submitting.html'><i class='layui-icon'>&#xe654;</i>上传实验报告</a>");
			} else {
                document.write("<span class='layui-breadcrumb'><a><cite>实验课程信息</cite></a></span>");
			}
		</script>
	</blockquote>
	<table class="layui-table" lay-data="{url:'__ROOT__/api/experiment/getExperimentList',page:true,limit:15, id:'reportList'}" lay-filter="reportList">
		<thead>
		<tr>
			<th lay-data="{type:'numbers', sort: true, fixed: 'left'}">ID</th>
			<th lay-data="{field:'experiment_name', width: 300}">实验名称</th>
			<th lay-data="{field:'experiment_main'}">实验内容</th>
			<th lay-data="{field:'experiment_address', width: 150}">实验地址</th>
			<th lay-data="{field:'start_time', sort: true, width: 200}">实验开始时间</th>
			<th lay-data="{field:'end_time', width: 200}">实验结束时间</th>
		</tr>
		</thead>
	</table>
</div>
<script src="__PUBLIC__/Home/js/jquery-1.9.1.min.js" type="text/javascript"></script>
<script src="__PUBLIC__/layui/layui.js"></script>
<script>
    layui.use(['form','table','upload','element'], function(){
        var table = layui.table
            ,form = layui.form
            ,upload = layui.upload
            ,element = layui.element;
    });
</script>
</body>
</html>